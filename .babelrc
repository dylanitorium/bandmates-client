{
  "presets": [
    "react-app",
  ],
  "plugins": [
    ["module-alias", [
      {
        "src": "./src/utils",
        "expose": "utils"
      },
      {
        "src": "./src/components",
        "expose": "components"
      },
      {
        "src": "./src/state",
        "expose": "state"
      },
      {
        "src": "./src/utils/HOC",
        "expose": "hoc"
      }
    ]]
  ]
}
